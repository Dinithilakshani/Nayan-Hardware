	<queryString language="SQL">
		<![CDATA[SELECT 
    od.orderId,
    od.description,
    od.qty,
    od.unitprice,
    od.amount,
    c.email,
    o.date
FROM 
    order_detail od
JOIN 
    orders o ON od.orderId = o.id
